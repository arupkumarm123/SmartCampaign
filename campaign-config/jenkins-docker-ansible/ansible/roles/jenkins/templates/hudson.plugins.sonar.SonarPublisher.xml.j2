<?xml version='1.0' encoding='UTF-8'?>
<hudson.plugins.sonar.SonarPublisher_-DescriptorImpl plugin="sonar@2.2.1">
    <installations>
        <hudson.plugins.sonar.SonarInstallation>
	    <name>campaign-service-sonar</name>
            <disabled>false</disabled>
	    <serverUrl>http://{{ sonar_web_host }}:{{ sonar_web_port }}</serverUrl>
            <mojoVersion></mojoVersion>
	    <databaseUrl>jdbc:mysql://{{ sonar_mysql_host }}:{{ sonar_mysql_port }}/{{ sonar_mysql_database}}?autoReconnect=true&amp;useUnicode=true&amp;characterEncoding=utf8</databaseUrl>
	    <databaseLogin>{{ sonar_mysql_user }}</databaseLogin>
            <additionalProperties></additionalProperties>
            <triggers>
                <skipScmCause>false</skipScmCause>
                <skipUpstreamCause>false</skipUpstreamCause>
                <envVar></envVar>
            </triggers>
	    <sonarLogin>{{ sonar_web_context }}</sonarLogin>
	    <databaseSecret>{{ sonar_mysql_password }}=</databaseSecret>
	    <sonarSecret>{{ sonar_web_user }}</sonarSecret>
        </hudson.plugins.sonar.SonarInstallation>
    </installations>
</hudson.plugins.sonar.SonarPublisher_-DescriptorImpl>

